KA9EA;AA+EA,IAAA,QA/EA,oBA+EA,IA/EA,EA+EA;AAAA,UAAA,KAAA,uEAAA,kCAAA;AACA,UAAA,IAAA,GAAA,QAAA,CAAA,aAAA,CAAA,KAAA,CAAA;AACA,MAAA,IAAA,CAAA,SAAA,GAAA,KAAA;AACA,UAAA,QAAA,sDAAA,IAAA,WAAA;AACA,MAAA,IAAA,CAAA,SAAA,GAAA,QAAA;AACA,aAAA,IAAA;AACA,KArFA;AAsFA,IAAA,YAtFA,wBAsFA,IAtFA,EAsFA;AACA,UAAA,OAAA,GAAA,IAAA;AACA,MAAA,IAAA;AACA,uBAAA,OAAA;AACA;AA1FA,GAtBA;AAkHA,EAAA,OAlHA,qBAkHA,CAAA,CAlHA;AAmHA,EAAA,OAnHA,qBAmHA;AACA,SAAA,cAAA,GAAA;AACA,MAAA,KAAA,EAAA,KAAA,eAAA,CAAA,KADA;AAEA,MAAA,IAAA,EAAA,KAAA,eAAA,CAAA;AAFA,KAAA;AAIA,GAxHA;AAyHA,EAAA,OAzHA,qBAyHA,CAAA,CAzHA;AA0HA,EAAA,QAAA,EAAA;AACA,IAAA,cAAA,EAAA;AACA,MAAA,GAAA,EAAA,eAAA;AACA,eAAA;AAAA,UAAA,KAAA,EAAA,KAAA,aAAA;AAAA,UAAA,IAAA,EAAA,KAAA;AAAA,SAAA;AACA,OAHA;AAIA,MAAA,GAAA,EAAA,aAAA,WAAA,EAAA;AACA,aAAA,YAAA,GAAA,WAAA,CAAA,IAAA;AACA,aAAA,aAAA,GAAA,WAAA,CAAA,KAAA;AACA;AAPA;AADA,GA1HA;AAqIA,EAAA,KAAA,EAAA;AACA,IAAA,cAAA,EAAA,0BAAA;AACA,WAAA,KAAA,CAAA,gBAAA,EAAA;AACA,QAAA,KAAA,EAAA,KAAA,aADA;AAEA,QAAA,IAAA,EAAA,KAAA;AAFA,OAAA;AAIA,WAAA,IAAA;AACA,KAPA;AAQA,IAAA,eAAA,EAAA,2BAAA;AACA,WAAA,cAAA,GAAA;AACA,QAAA,KAAA,EAAA,KAAA,eAAA,CAAA,KADA;AAEA,QAAA,IAAA,EAAA,KAAA,eAAA,CAAA;AAFA,OAAA;AAIA;AAbA;AArIA,CAAA,C,CAqJA;AACA;AACA;AACA;AACA","sourcesContent":["<template>\r\n  <div class=\"row\">\r\n    <div class=\"col\">\r\n      <div class=\"card rounded-0 shadow-sm\">\r\n        <div class=\"card-header bg-light\">\r\n          <div class=\"row\">\r\n            <div class=\"col\">\r\n              <span class=\"float-left\" @click=\"previous()\">\r\n                <i class=\"far fa-arrow-alt-circle-left arrow\"></i>\r\n              </span>\r\n            </div>\r\n            <div class=\"col\">{{ months[selectedMonth] }} - {{selectedYear}}</div>\r\n            <div class=\"col\">\r\n              <span class=\"float-right\" @click=\"next()\">\r\n                <i class=\"far fa-arrow-alt-circle-right arrow\"></i>\r\n              </span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"card-body\">\r\n          <div class=\"card-group cal-header\">\r\n            <div v-for=\"(day, i) in daysOfTheWeek\" :key=\"i\" class=\"card rounded-0\">\r\n              <span>{{day}}</span>\r\n            </div>\r\n          </div>\r\n          <div class=\"cal-body\"></div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"calendarUi\",\r\n  data() {\r\n    return {\r\n      selectedYear: null,\r\n      selectedMonth: null,\r\n      selectedDate: null,\r\n      hover: false,\r\n      weeks: 6,\r\n      date: 1,\r\n      firstDayOfTheMonth: new Date(\r\n        this.selectedYear,\r\n        this.selectedMonth\r\n      ).getDay()\r\n    };\r\n  },\r\n  props: {\r\n    currValues: Object,\r\n    months: Array,\r\n    selectedFromApp: Object,\r\n    daysOfTheWeek: Array\r\n  },\r\n  methods: {\r\n    createCalendar(month, year) {\r\n      // get first day of the month\r\n      const firstDay = new Date(year, month).getDay();\r\n      let section = document.querySelector(\".cal-body\");\r\n      let innerSection = document.createElement(\"div\");\r\n      let date = 1;\r\n      // loop through number of weeks\r\n      for (let i = 0; i < 6; i++) {\r\n        let row = document.createElement(\"div\");\r\n        row.className = \"card-group border-0\";\r\n        let isNotEmpty = false;\r\n        //loop through days of the week\r\n        for (let j = 0; j < 7; j++) {\r\n          if (i === 0 && j < firstDay) {\r\n            let card = this.makeCard(\"\");\r\n            row.appendChild(card);\r\n          } else if (date > this.daysInMonth(month, year)) {\r\n            if (j < 7) {\r\n              for (let x = j; x < 7; x++) {\r\n                let card = this.makeCard(\"\");\r\n                row.appendChild(card);\r\n              }\r\n            }\r\n            break;\r\n          } else {\r\n            isNotEmpty = true;\r\n            let card;\r\n            if (\r\n              date === this.currValues.date &&\r\n              month === this.currValues.month &&\r\n              year === this.currValues.year\r\n            ) {\r\n              card = this.makeCard(\r\n                date,\r\n                \"card card-cal-date text-left bg-info text-white p-1 rounded-0\"\r\n              );\r\n            } else {\r\n              card = this.makeCard(\r\n                date,\r\n                \"card card-cal-date text-left p-1 rounded-0 calday\"\r\n              );\r\n            }\r\n            row.appendChild(card);\r\n            date++;\r\n          }\r\n        }\r\n        if (isNotEmpty) {\r\n          innerSection.appendChild(row);\r\n        }\r\n      }\r\n      section.innerHTML = \"\";\r\n      section.appendChild(innerSection);\r\n      //console.log(firstDay);\r\n    },\r\n    daysInMonth(month, year) {\r\n      // gets the number of days within a month\r\n      return 32 - new Date(year, month, 32).getDate();\r\n    },\r\n    next() {\r\n      let year =\r\n        this.selectedMonth === 11 ? this.selectedYear + 1 : this.selectedYear;\r\n      let month = (this.selectedValues.month + 1) % 12;\r\n      this.selectedValues = { month: month, year: year };\r\n      this.createCalendar(this.selectedValues.month, this.selectedValues.year);\r\n    },\r\n    previous() {\r\n      let year =\r\n        this.selectedValues.month === 0\r\n          ? this.selectedValues.year - 1\r\n          : this.selectedValues.year;\r\n      let month =\r\n        this.selectedValues.month === 0 ? 11 : this.selectedValues.month - 1;\r\n      this.selectedValues = { month: month, year: year };\r\n      this.createCalendar(this.selectedValues.month, this.selectedValues.year);\r\n    },\r\n    jump() {\r\n      this.createCalendar(this.selectedValues.month, this.selectedValues.year);\r\n    },\r\n    makeCard(data, style = \"card card-cal-date p-1 rounded-0\") {\r\n      let card = document.createElement(\"div\");\r\n      card.className = style;\r\n      let cardBody = `<div class=\"card-body p-1 text-center\">${data}</div>`;\r\n      card.innerHTML = cardBody;\r\n      return card;\r\n    },\r\n    makeCardBody(date) {\r\n      const newData = date;\r\n      date++;\r\n      return `${newData}`;\r\n    }\r\n  },\r\n  created() {},\r\n  mounted() {\r\n    this.selectedValues = {\r\n      month: this.selectedFromApp.month,\r\n      year: this.selectedFromApp.year\r\n    };\r\n  },\r\n  updated() {},\r\n  computed: {\r\n    selectedValues: {\r\n      get: function() {\r\n        return { month: this.selectedMonth, year: this.selectedYear };\r\n      },\r\n      set: function(newSelected) {\r\n        this.selectedYear = newSelected.year;\r\n        this.selectedMonth = newSelected.month;\r\n      }\r\n    }\r\n  },\r\n  watch: {\r\n    selectedValues: function() {\r\n      this.$emit(\"selectedValues\", {\r\n        month: this.selectedMonth,\r\n        year: this.selectedYear\r\n      });\r\n      this.jump();\r\n    },\r\n    selectedFromApp: function() {\r\n      this.selectedValues = {\r\n        month: this.selectedFromApp.month,\r\n        year: this.selectedFromApp.year\r\n      };\r\n    }\r\n  }\r\n};\r\n//document.querySelector(\".calday\").onclick\r\n//document.querySelector(\".calday\").addEventListener(\"click\", function(e) {\r\n//e.target.style.background = \"#CCC\";\r\n//this.style.background-color = '#CCC';\r\n//});\r\n</script>\r\n\r\n<style>\r\n.table td {\r\n  height: 100px;\r\n}\r\n.card-group .cal-header .card {\r\n  font-size: 8px;\r\n}\r\n.card .card-cal-date .card-body {\r\n  height: 100px;\r\n}\r\n.card .calday:hover {\r\n  background-color: rgb(201, 200, 198);\r\n  color: white;\r\n  font-weight: 400;\r\n  cursor: pointer;\r\n}\r\n.card {\r\n  background-color: #f4f4f4;\r\n}\r\n.card-cal-date:hover {\r\n}\r\n.hover {\r\n  background-color: #333333;\r\n}\r\n.nohover {\r\n  background-color: #fff;\r\n}\r\n.arrowHover {\r\n  color: orange;\r\n}\r\n.arrow:hover {\r\n  color: orange;\r\n}\r\n@media (max-width: 575px) {\r\n  .display-4 {\r\n    font-size: 1.5rem;\r\n  }\r\n  .day h5 {\r\n    background-color: #f8f9fa;\r\n    padding: 3px 5px 5px;\r\n    margin: -8px -8px 8px -8px;\r\n  }\r\n  .date {\r\n    padding-left: 4px;\r\n  }\r\n}\r\n@media (min-width: 576px) {\r\n  .day {\r\n    height: 14.2857vw;\r\n  }\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}